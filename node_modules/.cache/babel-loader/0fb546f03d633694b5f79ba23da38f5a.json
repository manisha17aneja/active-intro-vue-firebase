AAA,IAAA,WAAA,GAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,KAJA;AAMA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AAEA,WAAA,EAAA,CAAA,MAAA;AACA,UAAA,KAAA,EAAA,CAAA,SAAA,IAAA,KAAA,EAAA,CAAA,MAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,GAAA,cAAA,EAAA,CAJA,CAIA;;AAGA,MAAA,EAAA,CAAA,UAAA,CAAA,MAAA,EACA,GADA,CACA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,MAAA,EAAA,KAAA,MAFA;AAGA,QAAA,OAAA,EAAA,IAHA;AAIA,QAAA,QAAA,EAAA;AAJA,OADA,EAOA,IAPA,CAOA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,qBAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,8BAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA,CAEA;;AAFA,SAAA;;AAIA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA;AACA,OAdA,EAeA,KAfA,CAeA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,0BAAA,EAAA,KAAA;AACA,OAjBA;AAmBA;AAhCA;AAnBA,CAAA","sourcesContent":["<template>\n\n\n \n<body>\n\n\n<div id=\"main-wrapper\" data-layout=\"vertical\" data-navbarbg=\"skin5\" data-sidebartype=\"full\"\n    data-sidebar-position=\"absolute\" data-header-position=\"absolute\" data-boxed-layout=\"full\">\n\n    <Header></Header>\n\n\n\n    <Sidebar></Sidebar>\n\n    <div class=\"dashboard-wrapper page-wrapper\">\n            <div class=\"page-breadcrumb\">\n                <div class=\"row align-items-center\">\n                    <div class=\"col-lg-3 col-md-4 col-sm-4 col-xs-12\">\n                        <h4 class=\"page-title\">Create FAQ</h4>\n                    </div>\n                </div>\n            </div>\n                        <div class=\"container-fluid\">\n                <div class=\"row\">\n             \n                    <div class=\"col-lg-12 col-xlg-9 col-md-12\">\n\n                    <form class=\"form-horizontal form-material\" v-on:submit.prevent=\"submit\">\n\n\n                        <div class=\"card profile-card\">\n                            <div class=\"card-body\">\n                               \n            \n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Question</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" v-model.trim=\"$v.question.$model\" :class=\"{'is-invalid': validationStatus($v.question)}\" class=\"form-control form-control-lg\">\n                                            <div v-if=\"!$v.question.required\" class=\"invalid-feedback\">The question field is required.</div>\n                                        </div>\n                                    </div>\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Answer</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <textarea v-model.trim=\"$v.answer.$model\" :class=\"{'is-invalid': validationStatus($v.answer)}\" class=\"form-control form-control-lg\" rows=\"5\"></textarea>\n                                            <div v-if=\"!$v.answer.required\" class=\"invalid-feedback\">The answer field is required.</div>\n                                        </div>\n                                     </div>\n\n                                   \n                                    <div class=\"form-group mb-4\">\n                                        <div class=\"col-sm-12 pl-0 mt-10\">\n                                        <input type=\"submit\" class=\"btn btn-primary\" value=\"Submit\"/>\n                                        </div>\n                                    </div>\n\n                                </div>\n                            </div>\n                            </form>\n                        \n\n                        \n                    </div>\n                </div>\n            </div>\n           \n        </div>\n    \n    <Footer></Footer>\n\n</div>\n\n\n\n\n\n</body>\n\n\n</template>\n\n\n\n\n<script>\n\nimport Header from './../admin-header';\nimport Footer from './../admin-footer';\nimport Sidebar from './../admin-sidebar';\n\nimport { required } from 'vuelidate/lib/validators'\nimport Vue from 'vue'\nimport axios from 'axios'\nimport VueAxios from 'vue-axios'\nimport { db } from '../../main'\n\nVue.use(VueAxios,axios)\n\n\n\nexport default {\n    name: 'Create',\n    components:{\n        'Header':Header,\n        'Sidebar':Sidebar,\n        'Footer':Footer,\n       \n    },\n    data: function() {\n        return {\n            question: '',\n            answer: '',\n        }\n    }, \n    validations: {\n        question: {required},\n        answer: {required},\n    },\n\n    methods: {\n\n        validationStatus: function(validation) {\n            return typeof validation != \"undefined\" ? validation.$error : false;\n        },\n\n        submit: function() {\n\n            this.$v.$touch();\n            if (this.$v.$pendding || this.$v.$error) return;\n            var  date = new Date().toLocaleString();//.toISOString().slice(0, 10);\n\n            \n            db.collection(\"faqs\")\n            .add({\n                question: this.question,\n                answer: this.answer,\n                created: date,\n                modified: date,\n            })\n            .then(() => {\n                console.log(\"Added Successfully!\");\n                this.$toasted.success('Success, Added Successfully!', {\n                   icon : 'check',\n                    //icon : 'error_outline'\n                })\n                this.$router.push('/faqs/index');\n            })\n            .catch((error) => {\n                console.error(\"Error writing document: \", error);\n            }); \n\n        }\n    }\n}\n</script>"],"sourceRoot":"src/components/faqs"}]}