A0BA,IAAA,WA1BA,uBA0BA,CA1BA,EA0BA;AAAA;;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,CAAA,KAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,CAAA,CAAA,MAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OAHA;AAIA,KAlCA;AAmCA,IAAA,OAnCA,qBAoCA;AAAA;;AACA,UAAA,EAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,eAAA,EAAA,GAAA,CAAA,EAAA,EAAA,GAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAJA;AAKA,KA5CA;AA8CA,IAAA,MA9CA,oBA+CA;AAAA;;AACA,UAAA,EAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,GAAA,cAAA,EAAA,CAFA,CAEA;;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,eAAA,EACA,GADA,CACA,EADA,EAEA,MAFA,CAEA;AACA,QAAA,OAAA,EAAA,KAAA,IAAA,CAAA,OADA;AAEA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KAFA;AAGA,QAAA,WAAA,EAAA,KAAA,IAAA,CAAA,WAHA;AAIA,QAAA,GAAA,EAAA,KAAA,IAAA,CAAA,GAJA;AAKA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KALA;AAMA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QANA;AAOA,QAAA,GAAA,EAAA,KAAA,IAAA,CAAA,GAPA;AAQA,QAAA,UAAA,EAAA,KAAA,IAAA,CAAA,UARA;AASA,QAAA,gBAAA,EAAA,KAAA,IAAA,CAAA,gBATA;AAUA,QAAA,aAAA,EAAA,KAAA,IAAA,CAAA,aAVA;AAWA,QAAA,KAAA,EAAA,KAAA,YAXA;AAYA,QAAA,QAAA,EAAA;AAZA,OAFA,EAgBA,IAhBA,CAgBA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,gCAAA,EADA,CAEA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,gCAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA,CAEA;;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,sBAAA;AAEA,OAzBA,EA0BA,KA1BA,CA0BA,UAAA,KAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,2BAAA,EAAA,KAAA;AACA,OA7BA;AA8BA;AAjFA;AArBA,CAAA","sourcesContent":["<template>\n\n\n \n<body>\n\n\n<div id=\"main-wrapper\" data-layout=\"vertical\" data-navbarbg=\"skin5\" data-sidebartype=\"full\"\n    data-sidebar-position=\"absolute\" data-header-position=\"absolute\" data-boxed-layout=\"full\">\n\n    <Header></Header>\n\n\n\n    <Sidebar></Sidebar>\n\n    <div class=\"page-wrapper\">\n            <div class=\"page-breadcrumb\">\n                <div class=\"row align-items-center\">\n                    <div class=\"col-lg-3 col-md-4 col-sm-4 col-xs-12\">\n                        <h4 class=\"page-title\">Edit Subscription</h4>\n                    </div>\n                </div>\n            </div>\n                        <div class=\"container-fluid\">\n                <div class=\"row\">\n             \n                    <div class=\"col-lg-12 col-xlg-9 col-md-12\">\n                       \n                            <form class=\"form-horizontal form-material\" v-on:submit.prevent=\"submit\">\n\n                                <div class=\"card profile-card\">\n                                    <div class=\"card-body\">\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Country</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                        <select name=\"country\" v-model=\"item.country\" class=\"form-control\">\n                                            <option v-for=\"option in options\" v-bind:key=\"option.name\" v-bind:value=\"option.id\">\n                                                {{ option.name }}\n                                            </option>\n                                        </select>\n                                       <!-- <span>Selected: {{ item.country }}</span>--->\n                                        </div>\n                                    </div>\n            \n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Title</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"title\" v-model=\"item.title\"/>\n                                        </div>\n                                    </div>\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Description</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <textarea  name=\"description\" v-model=\"item.description\" class=\"form-control form-control-lg\" rows=\"5\"></textarea>\n                                        </div>\n                                    </div>\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Tag Line</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"tag\" v-model=\"item.tag\"/>\n                                        </div>\n                                    </div>\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Image</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                        <img v-bind:src=\"previewImage\" class=\"uploading-image\" />\n                                        <input type=\"file\" accept=\"image/jpeg\" @change=uploadImage>\n                                         \n                                        </div>\n                                    </div>\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Price</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"price\" v-model=\"item.price\"/>\n                                        </div>\n                                    </div>\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Discount (%)</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"discount\" v-model=\"item.discount\"/>\n                                        </div>\n                                    </div>\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Vat (%)</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"vat\" v-model=\"item.vat\"/>\n                                        </div>\n                                    </div>\n\n                                    </div>\n                                    </div>\n\n                                    <div class=\"card profile-card\">\n                                    <div class=\"card-body\">\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Meta Title</label>\n                                        <div class=\"col-md-12 border-bottom  p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"\" v-model=\"item.meta_title\"/>\n                                        </div>\n                                    </div>\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Meta Description</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <textarea  name=\"meta_description\" v-model=\"item.meta_description\" class=\"form-control form-control-lg\" rows=\"5\"></textarea>\n                                        </div>\n                                    </div>\n\n\n                                    <div class=\"form-group mb-4\">\n                                        <label class=\"col-md-12 p-0\">Meta Keywords</label>\n                                        <div class=\"col-md-12 border-bottom p-0\">\n                                            <input type=\"text\" class=\"form-control\" name=\"meta_keywords\" v-model=\"item.meta_keywords\"/>\n                                        </div>\n                                    </div>\n\n\n                                    <br>\n                                    <div class=\"form-group mb-4\">\n                                        <div class=\"col-sm-12\">\n                                        <input type=\"submit\" class=\"btn btn-primary\" value=\"Submit\"/>\n                                        </div>\n                                    </div>\n\n                                </div>\n                            </div>\n                        </form> \n                    </div>\n                </div>\n            </div>\n           \n        </div>\n    \n    <Footer></Footer>\n\n</div>\n\n\n\n\n\n</body>\n\n\n</template>\n\n\n\n\n\n\n\n<script>\n\nimport Header from './../admin-header';\nimport Footer from './../admin-footer';\nimport Sidebar from './../admin-sidebar';\n\nimport { db } from '../../main'\n\nexport default{\n     name: 'Edit',\n        data(){\n            return{\n                item:{},\n                previewImage:'',\n                options: [],\n            }\n        },\n        components:{\n            'Header':Header,\n            'Sidebar':Sidebar,\n            'Footer':Footer,\n        \n        },\n\n        created: function(){\n            this.getItem();\n            this.getCountries();\n        },\n\n        methods: {\n\n            getCountries()\n            {\n                this.options = [];\n\n                db.collection(\"countries\")\n                .get()\n                .then((querySnapshot) => {\n                    //console.log('hhhh');\n                    querySnapshot.forEach((doc) => {\n                    this.options.push({\n                        id: doc.id,\n                        name: doc.data().country,\n                    });\n                    //console.log(doc.id, \" => \", doc.data());\n                    \n                    });\n\n                    //console.log(this.options);\n                })\n                .catch((error) => {\n                    console.log(\"Error getting documents: \", error);\n                });\n            },\n\n            uploadImage(e){\n                const image = e.target.files[0];\n                const reader = new FileReader();\n                reader.readAsDataURL(image);\n                reader.onload = e =>{\n                    this.previewImage = e.target.result;\n                    console.log('dd');\n                };\n            },\n            getItem()\n            {\n                var id = this.$route.params.id;\n\n                db.collection('subscriptions').doc(id).get().then((doc) => {\n                    console.log(doc.data());\n                    this.item = doc.data();\n                    this.previewImage = doc.data().image;\n                })\n            },\n\n            submit()\n            {\n                var id = this.$route.params.id;\n                var  date = new Date().toLocaleString();//.toISOString().slice(0, 10);\n\n                db.collection(\"subscriptions\")\n                .doc(id)\n                .update({\n                  country: this.item.country,\n                  title: this.item.title,\n                  description: this.item.description,\n                  tag: this.item.tag,\n                  price: this.item.price,\n                  discount: this.item.discount,\n                  vat: this.item.vat,\n                  meta_title: this.item.meta_title,\n                  meta_description: this.item.meta_description,\n                  meta_keywords: this.item.meta_keywords,\n                  image:this.previewImage,\n                  modified: date,\n                })\n                .then(() => {\n                  console.log(\"Document successfully updated!\");\n                  //this.getItem();\n                  this.$toasted.success('Success, Updated Successfully!', {\n                    icon : 'check',\n                     //icon : 'error_outline'\n                 })\n                 this.$router.push('/subscriptions/index');\n                 \n                })\n                .catch((error) => {\n                  // The document probably doesn't exist.\n                  console.error(\"Error updating document: \", error);\n                });\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/components/subscriptions"}]}