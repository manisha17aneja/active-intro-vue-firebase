AAA,CAAA,IAAA,CAAA;AACA,YAAA,KAAA,EAAA,CADA;AAEA,YAAA,EAAA,EAAA,GAAA,CAAA,EAFA;AAGA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,GAAA,OAHA;AAIA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA,KAJA;AAKA,YAAA,iBAAA,EAAA,GAAA,CAAA,IAAA,GAAA,iBALA;AAMA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA,KANA;AAOA,YAAA,UAAA,EAAA,GAAA,CAAA,IAAA,GAAA,UAPA,CAQA;;AARA,WAAA;;AAUA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,MAAA,EAAA,GAAA,CAAA,IAAA,EAAA;AACA,UAAA,CAAA;AACA,SAbA;AAcA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,EAAA,KAAA;AACA,OArBA;AAsBA,KA3BA;AA4BA,IAAA,cA5BA,0BA4BA,EA5BA,EA4BA;AAAA;;AAEA,UAAA,OAAA,CAAA,+BAAA,CAAA,EAAA;AACA;AACA,QAAA,EAAA,CAAA,UAAA,CAAA,QAAA,EACA,GADA,CACA,EADA,EAEA,MAFA,GAGA,IAHA,CAGA,YAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gCAAA;;AACA,UAAA,MAAA,CAAA,aAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,gCAAA,EAAA;AACA,YAAA,IAAA,EAAA,OADA,CAEA;;AAFA,WAAA,EAHA,CAOA;;AAEA,SAZA,EAaA,KAbA,CAaA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,2BAAA,EAAA,KAAA;AACA,SAfA;AAiBA;AACA;AAlDA,GAhBA;AAoEA,EAAA,OApEA,qBAoEA;AACA,SAAA,aAAA;AACA;AAtEA,CAAA","sourcesContent":["<template>\n\n\n \n<body>\n\n\n<div id=\"main-wrapper\" data-layout=\"vertical\" data-navbarbg=\"skin5\" data-sidebartype=\"full\"\n    data-sidebar-position=\"absolute\" data-header-position=\"absolute\" data-boxed-layout=\"full\">\n\n    <Header></Header>\n\n\n\n    <Sidebar></Sidebar>\n\n    <div class=\"dashboard-wrapper page-wrapper\">\n\n        <div class=\"page-breadcrumb\">\n            <div class=\"row align-items-center\">\n                <div class=\"col-lg-3 col-md-4 col-sm-4 col-xs-12\">\n                    <h4 class=\"page-title\">List Offers</h4>\n                </div>\n            </div>\n        </div>\n\n\n        <div class=\"container-fluid\">\n            <div class=\"row\">\n                <div class=\"col-sm-12\">\n                    <div class=\"white-box\">\n                        <div class=\"uper-add-row\">                       \n                             <h3 class=\"box-title\">List Offers</h3>\n                            <el-button\n                            size=\"mini\"\n                            type=\"success\"\n                            @click=\"$router.push({ path: '/offers/create'})\"\n                            >+ Add</el-button>\n                        </div>\n                        <div class=\"table-responsive\">\n                         \n\n\n                            <el-table\n      :data=\"\n      resultData.filter(\n          (data) =>\n            !search || data.name.toLowerCase().includes(search.toLowerCase())\n        )\n      \"\n      style=\"width: 100%;\"\n    >\n       <el-table-column label=\"Sr No.\" prop=\"index\"> </el-table-column>\n      <!--<el-table-column label=\"Id\" prop=\"id\"> </el-table-column>--->\n      <el-table-column label=\"Company\" prop=\"company\"> </el-table-column>\n      <el-table-column label=\"Title\" prop=\"title\"> </el-table-column>\n      <el-table-column label=\"Short Description\" prop=\"short_description\"> </el-table-column>\n\n      <el-table-column min-width=\"55\"  prop=\"image\" label=\"Image\">\n        <template slot-scope=\"scope\">\n            <img :src=\"scope.row.image\"/>\n        </template>\n      </el-table-column>\n      \n      <el-table-column label=\"Valid until\" prop=\"valid_date\"> </el-table-column>\n\n\n\n      \n      <el-table-column align=\"right\">\n          \n        <template slot-scope=\"scope\">\n        <el-button\n            size=\"mini\"\n            type=\"info\"\n            @click=\"$router.push({ path: '/offers/edit/'+scope.row.id})\"\n            >Edit</el-button>\n\n          <el-button\n            size=\"mini\"\n            type=\"danger\"\n            @click=\"deleteEmployee(scope.row.id)\"\n            >Delete</el-button>\n\n        </template>\n      </el-table-column>\n    </el-table>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n        </div>\n\n\n    </div>\n    \n    <Footer></Footer>\n\n</div>\n\n\n\n\n\n</body>\n\n\n</template>\n\n\n\n<script>\n\nimport Header from './../admin-header';\nimport Footer from './../admin-footer';\nimport Sidebar from './../admin-sidebar';\n\n\nimport Vue from 'vue'\nimport axios from 'axios'\nimport VueAxios from 'vue-axios'\nimport { db } from '../../main'\n\nVue.use(VueAxios,axios)\n\nexport default {\n    components:{\n        'Header':Header,\n        'Sidebar':Sidebar,\n        'Footer':Footer,\n       \n    },\n  data() {\n    return {\n      name: \"\",\n      about: \"\",\n      date: new Date().toISOString().slice(0, 10),\n      resultData: [],\n      search: \"\",\n    };\n  },\n  methods: {\n\n    readEmployees() {\n      this.resultData = [];\n      var i = 1;\n      db.collection(\"offers\")\n        .get()\n        .then((querySnapshot) => {\n         // console.log('hhhh');\n          querySnapshot.forEach((doc) => {\n            this.resultData.push({\n              index: i,\n              id: doc.id,\n              company: doc.data().company,\n              title: doc.data().title,\n              short_description: doc.data().short_description,\n              image: doc.data().image,\n              valid_date: doc.data().valid_date,\n              //modified: doc.data().modified,\n            });\n            console.log(doc.id, \" => \", doc.data());\n            i++;\n          });\n        })\n        .catch((error) => {\n          console.log(\"Error getting documents: \", error);\n        });\n    },\n    deleteEmployee(id) {\n\nif(confirm(\"Do you really want to delete?\")){\n       // alert('dddd');\n      db.collection(\"offers\")\n        .doc(id)\n       .delete()\n        .then(() => {\n          console.log(\"Document successfully deleted!\");\n          this.readEmployees();\n          this.$toasted.success('Success, Deleted Successfully!', {\n            icon : 'check',\n             //icon : 'error_outline'\n         })\n         //this.$router.push('/offers/index');\n          \n        })\n        .catch((error) => {\n          console.error(\"Error removing document: \", error);\n        });\n\n    }\n    },\n  },\n  mounted() {\n    this.readEmployees();\n  },\n};\n</script>"],"sourceRoot":"src/components/offers"}]}